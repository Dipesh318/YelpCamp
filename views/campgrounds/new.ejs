<%- include("../partials/header") %>
<div class="container">
	<div class="row">
		<h1 style="text-align:center">Add New CampGround</h1>
		
		<div class="forms">
			<form action="/campground" method="POST">
				<div class="form-group">
					<input class="form-control" type="text" name="campName" placeholder="CampGround Name" required>
				</div>
				<div class="form-group">
					<input class="form-control" type="number" name="price" placeholder="$XX.XX" min="10" required>
				</div>
				<div class="form-group">
					<input class="form-control" type="url" name="imageURL" placeholder="Image URL" required>	
				</div>
				<div class="form-group">
					<input class="form-control form-group" type="text" name="desc" placeholder="Description" required>
				</div>
				<div class="form-group">
					<button class="btn btn-primary btn-lg btn-block form-group">submit</button>
				</div>
				
			</form>
		</div>
		
	</div>

</div>
	
<%- include("../partials/footer") %>